{
  "name": "webman-tech/laravel-validation",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin webman-tech/laravel-validation",
  "require": {
    "php": ">=7.2",
    "illuminate/validation": ">=7.0"
  },
  "require-dev": {
    "illuminate/database": ">=7.0",
    "phpunit/phpunit": "^9",
    "symfony/translation": ">=5.0",
    "webman-tech/laravel-translation": "^1.0",
    "workerman/webman-framework": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "WebmanTech\\LaravelValidation\\": "src"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WebmanTech\\LaravelValidation\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit --testdox --no-interaction"
  }
}
